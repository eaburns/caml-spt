;;
;; This is a sample spt-it-out plot
;;
(let (
      (ds0 (scatter-dataset
	    (name "ds0")
	    (color 0 0.8 0)
	    (points (0 0) (1 1) (2 2) (5 3) (3 0.7))))
      (ds1 (line-points-dataset
	    (dashes ((pt 2) (pt 2)))
	    (color 0.8 0 0)
	    (name "ds1")
	    (points (0 2) (1 0) (2 -2))))
      (ds2 (line-points-dataset
	    (dashes ((pt 2) (pt 2)))
	    (color 0 0 0.8)
	    (name "ds2")
	    (points (0.5 0) (4.1 3.14) (6.2 4))))
      )
  (let (
	(plot0 (num-by-num-plot
		(title "Test plot")
		(x-label "The x variable")
		(y-label "The y variable")
		(ds0 ds1 ds2)))
	)
    (display plot0)))
